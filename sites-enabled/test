handle_path /vendor* {
    import auth
    reverse_proxy 172.16.1.251:7007 {
        header_up X-Forwarded-Host {http.request.header.X-Forwarded-Host}
    }
}


handle_path /* {
    import auth
    reverse_proxy 172.16.1.251:8005 {
        header_up X-Forwarded-Host {http.request.header.X-Forwarded-Host}
        #header_up X-Username {http.request.header.X-Authentik-Username}
    }
}